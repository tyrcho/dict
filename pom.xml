<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>info.daviot</groupId>
	<artifactId>Dictionary</artifactId>
	<version>1.0.0</version>
	<name>Dictionary</name>
	<dependencies>
		<dependency>
			<groupId>com.thoughtworks.xstream</groupId>
			<artifactId>xstream</artifactId>
			<version>1.4.2</version>
		</dependency>
		<dependency>
			<groupId>jasperreports</groupId>
			<artifactId>jasperreports</artifactId>
			<version>2.0.5</version>
		</dependency>
		<dependency>
			<groupId>info.daviot</groupId>
			<artifactId>JavaCommonTools</artifactId>
			<version>1.0.0</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<!-- must be run with a JDK (not a JRE) and is started with mvn package 
					webstart:jnlp -->
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>webstart-maven-plugin</artifactId>
				<version>1.0-beta-3</version>
				<executions>
					<execution>
						<phase>package</phase>
						<goals>
							<goal>jnlp</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<excludeTransitive>false</excludeTransitive>
					<jnlp>
						<outputFile>dict.jnlp</outputFile>
						<mainClass>com.tyrcho.dictionary.view.DictionaryFrame</mainClass>
					</jnlp>
					<sign>
						<keystore>tyrcho</keystore>
						<keypass>password</keypass>
						<storepass>password</storepass>
						<alias>tyrcho</alias>
						<verify>true</verify>
						<dnameCn>Michel Daviot</dnameCn>
						<dnameOu></dnameOu>
						<dnameO></dnameO>
						<dnameL>Lille</dnameL>
						<dnameSt>France</dnameSt>
						<dnameC>FR</dnameC>
						<validity>3650</validity>
						<keystoreConfig>
							<gen>true</gen>
						</keystoreConfig>
					</sign>
					<verbose>true</verbose>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<organization>
		<name>daviot.info</name>
	</organization>
	<url>file:///D:\dev\java\workspace\Dictionary\target</url>
</project> 